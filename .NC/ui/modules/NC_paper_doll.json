                ],
                "bindings": [
                  {
                    "binding_type": "view",
                    "source_control_name": "2d_paper_doll_toggle",
                    "source_property_name": "#toggle_state",
                    "target_property_name": "#visible"
                  }
                ],
                "layer": 1,
                "renderer": "live_player_renderer",
                "type": "custom"
              }
            }
          ],
          "size": [
            15,
            15
          ],
          "type": "panel"
        }
      }
    ]
  }
