{
	"namespace": "Nova_Client_trade",
	"common_panel_auto": {
	  "type": "panel",
	  "size": [
		"100%",
		"100%"
	  ],
	  "$cell_selected_ignored": true,
	  "$player_inventory_ignored|default": true,
	  "controls": [
		{
		  "edit_drop_button": {
			"type": "stack_panel",
			"anchor_from": "right_middle",
			"anchor_to": "right_middle",
			"size": [
			  61,
			  18
			],
			"offset": "$correct_offset",
			"$correct_offset|default": [
			  -5,
			  -6
			],
			"layer": 10,
			"orientation": "horizontal",
			"controls": [
			  {
				"text_edit_control": {
				  "type": "panel",
				  "size": [
					35,
					17
				  ],
				  "controls": [
					{
					  "text_edit_control@Nova_Client_trade.text_edit_control": {
						"anchor_from": "center",
						"anchor_to": "center",
						"offset": [
						  -3,
						  0
						],
						"size": [
						  "100% + 4px",
						  16
						]
					  }
					}
				  ]
				}
			  },
			  {
				"drop_auto_all_items@Nova_Client_trade.drop_auto_all_items": {
				  "size": [
					25,
					17
				  ]
				}
			  }
			]
		  }
		}
	  ]
	},
	"auto_items_template": {
	  "type": "panel",
	  "$auto_size|default": [
		"100%",
		"100%"
	  ],
	  "size": "$auto_size",
	  "$inf_hover_button_ignored|default": true,
	  "$cell_add|default": "Nova_Client_trade.cell_button",
	  "$super_collection_name|default": "inventory_items",
	  "$super_grid_item_template|default": "common.auto_grid_item_for_inventory",
	  "controls": [
		{
		  "$cell_add@$cell_add": {}
		},
		{
		  "inf_hover_button@common.inf_hover_button": {
			"ignored": "$inf_hover_button_ignored",
			"$hover_label": "Click to move items quickly"
		  }
		},
		{
		  "grid": {
			"type": "panel",
			"size": [
			  "100%",
			  "100%"
			],
			"anchor_from": "top_middle",
			"anchor_to": "top_middle",
			"controls": [
			  {
				"inv_interface": {
				  "type": "grid",
				  "anchor_from": "center",
				  "anchor_to": "center",
				  "$super_mode": true,
				  "$cell_controls_visible": true,
				  "size": [
					0,
					0
				  ],
				  "offset": [
					-9,
					-9
				  ],
				  "layer": 0,
				  "grid_dimensions": [
					1,
					54
				  ],
				  "grid_item_template": "$super_grid_item_template",
				  "collection_name": "$super_collection_name"
				}
			  }
			]
		  }
		}
	  ]
	},
	"cell_button": {
	  "type": "panel",
	  "size": [
		"100%",
		"100%"
	  ],
	  "layer": 20,
	  "controls": [
		{
		  "label": {
			"type": "label",
			"size": [
			  "default",
			  "default"
			],
			"layer": 1005,
			"color": "white",
			"anchor_from": "center",
			"anchor_to": "center",
			"text": "$text"
		  }
		}
	  ]
	},
	"container_auto_place": {
	  "type": "panel",
	  "controls": [
		{
		  "container_auto_place@common.container_slot_button_prototype": {
			"$cell_selected_ignored": true,
			"$to_button_id|default": "button.container_auto_place",
			"button_mappings": [
			  {
				"from_button_id": "button.menu_select",
				"to_button_id": "$to_button_id",
				"mapping_type": "pressed"
			  }
			]
		  }
		}
	  ],
	  "bindings": [
		{
		  "binding_name": "#hover_text",
		  "binding_name_override": "#current_item_name",
		  "binding_type": "collection",
		  "binding_collection_name": "$item_collection_name"
		},
		{
		  "binding_type": "view",
		  "source_control_name": "search_cell_item",
		  "source_property_name": "#item_name",
		  "target_property_name": "#search_text"
		},
		{
		  "binding_type": "view",
		  "source_property_name": "(#search_text = '')",
		  "target_property_name": "#air_search"
		},
		{
		  "binding_type": "view",
		  "source_property_name": "(not ((#current_item_name - #search_text) = #current_item_name))",
		  "target_property_name": "#search_result"
		},
		{
		  "binding_type": "view",
		  "source_property_name": "(#search_result or #air_search)",
		  "target_property_name": "#visible"
		}
	  ],
	  "property_bag": {
		"#air_search": true,
		"#search_result": false
	  }
	},
	"text_edit_control@common.text_edit_box": {
	  "size": [
		"100%",
		"100%"
	  ],
	  "max_length": 300,
	  "$text_edit_text_control": "search_cell_item"
	},
	"drop_auto_all_items": {
	  "type": "panel",
	  "$add_button|default": "common.empty",
	  "$controls_button|default": "Nova_Client_trade.common_panel_auto_items_template",
	  "controls": [
		{
		  "add@$add_button": {}
		},
		{
		  "auto_items_template@$controls_button": {}
		}
	  ]
	},
	"common_panel_auto_items_template@Nova_Client_trade.auto_items_template": {
	  "$cell_add|default": "Nova_Client_trade.trash_default",
	  "size": "$auto_size",
	  "$auto_size": [
		"100%",
		"100%"
	  ],
	  "$button_ref": "Nova_Client_trade.drop_all",
	  "$super_collection_name": "inventory_items",
	  "$super_grid_item_template": "common.auto_grid_item_for_inventory"
	},
	"drop_all@Nova_Client_trade.tab_button_id_toggle": {
	  "$source_property_name": "(not (#item_id_aux / 65536) = 0)",
	  "$to_button_id": "button.drop_all"
	},
	"tab_button_id_toggle": {
	  "type": "panel",
	  "controls": [
		{
		  "tab_button_id_toggle@Nova_Client_trade.auto_item_template": {
			"$to_button_id|default": "button.container_auto_place"
		  }
		}
	  ],
	  "bindings": [
		{
		  "binding_name": "#hover_text",
		  "binding_name_override": "#current_item_name",
		  "binding_type": "collection",
		  "binding_collection_name": "$item_collection_name"
		},
		{
		  "binding_type": "view",
		  "source_control_name": "search_cell_item",
		  "source_property_name": "#item_name",
		  "target_property_name": "#search_text"
		},
		{
		  "binding_type": "view",
		  "source_property_name": "(#search_text = '')",
		  "target_property_name": "#air_search"
		},
		{
		  "binding_type": "view",
		  "source_property_name": "(not ((#current_item_name - #search_text) = #current_item_name))",
		  "target_property_name": "#search_result"
		},
		{
		  "binding_type": "view",
		  "source_property_name": "(#search_result or #air_search)",
		  "target_property_name": "#visible"
		}
	  ],
	  "property_bag": {
		"#air_search": true,
		"#search_result": false
	  }
	},
	"auto_item_template@common.container_slot_button_prototype": {
	  "size": "$container_button",
	  "$container_button|default": [
		"100%",
		"100%"
	  ],
	  "button_mappings": [
		{
		  "from_button_id": "button.menu_select",
		  "to_button_id": "$to_button_id",
		  "mapping_type": "pressed"
		}
	  ]
	},
	"auto_output_tertiary@common.inf_hover_button": {
	  "ignored": "(not $touch)",
	  "size": [
		"100%",
		"100%"
	  ],
	  "offset": [
		0,
		-17
	  ],
	  "$button_image_ignored": false,
	  "$hover_background_renderer_ignored": false,
	  "$textures": "textures/ui/hammer_r",
	  "$zoom_size": [
		"100% - 2px",
		"100% - 2px"
	  ],
	  "$hover_label": "Output Tertiary",
	  "anchor_from": "bottom_right",
	  "anchor_to": "bottom_right",
	  "$to_button_id": "button.crafting_output_tertiary",
	  "property_bag": {
		"#collection_name": "crafting_output_items",
		"#collection_index": 0
	  }
	},
	"trash_default": {
	  "type": "panel",
	  "anchor_from": "center",
	  "anchor_to": "center",
	  "alpha": 1.0,
	  "size": [
		"100%",
		"100%"
	  ],
	  "controls": [
		{
		  "trash_default_image@Nova_Client_common.external": {
			"$NC_external": "trash_default",
			"anchor_from": "center",
			"anchor_to": "center",
			"alpha": 1.0,
			"size": [
			  "100% - 2px",
			  "100% - 2px"
			]
		  }
		},
		{
		  "inf_hover_button@common.inf_hover_button": {
			"$hover_label": "Click to drop marked items",
			"$hover_background_renderer_ignored": false
		  }
		}
	  ]
	}
  }