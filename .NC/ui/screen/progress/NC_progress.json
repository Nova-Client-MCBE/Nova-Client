{
  "namespace": "Nova_Client_progress_screen",
  "main_loading_panel@Nova_Client_common.external": {
    "$NC_external": "hud_tip_text_background",
    "anchor_from": "center",
    "anchor_to": "center",
    "layer": 0,
    "alpha": 0.5,
    "size": [
      150,
      150
    ],
    "controls": [
      {
        "pick_axe@Nova_Client_common.external": {
          "layer": 1,
          "$NC_external": "mine_chop_dig_animation",
          "size": [
            50,
            50
          ],
          "uv_size": [
            10,
            10
          ],
          "fps": 20,
          "uv": "@persona.pick_axe_animation",
          "color": [
            1,
            1,
            1,
            1
          ]
        }
      },
      {
        "@Nova_Client_progress_percentage.content": {
          "text_alignment": "center",
          "anchor_from": "center",
          "anchor_to": "center",
          "size": [
            25,
            "default"
          ],
          "offset": [
            0,
            60
          ]
        }
      }
    ]
  },
  "title@Nova_Client_common.external": {
    "$NC_external": "title",
    "layer": 1,
    "anchor_from": "center",
    "anchor_to": "center",
    "offset": [
      0,
      "-30%"
    ],
    "size": [
      "45%",
      "25%"
    ]
  },
  "exit_button@Nova_client_button.clear_button": {
    "$button_text": "<",
    "$pressed_button_name": "button.menu_exit",
    "$button_text_binding_type": "global",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "layer": 3,
    "size": [
      21,
      21
    ],
    "offset": [
      1,
      1
    ],
    "$$text_color": [
      1,
      1,
      1
    ],
    "bindings": [
      {
        "binding_name": "#cancel_visible",
        "binding_name_override": "#visible"
      }
    ]
  },
  "content": {
    "type": "panel",
    "controls": [
      {
        "main@Nova_Client_progress_screen.main_loading_panel": {}
      },
      {
        "title@Nova_Client_progress_screen.title": {}
      },
      {
        "exit_button@Nova_Client_progress_screen.exit_button": {}
      },
      {
        "black@Nova_Client_common.black": {}
      }
    ]
  }
}