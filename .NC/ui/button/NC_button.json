  "default@Nova_client_button.button_content": {
    "$default": true,
    "texture": "$default_texture"
  },
  "hover@Nova_client_button.button_content": {
    "$hover": true,
    "texture": "$hover_texture"
  },
  "pressed@Nova_client_button.button_content": {
    "$pressed": true,
    "texture": "$pressed_texture"
  },
  "locked@Nova_client_button.button_content": {
    "$locked": true,
    "texture": "$locked_texture"
  },
  "button_content": {
    "type": "image",
    "$default|default": false,
    "$hover|default": false,
    "$pressed|default": false,
    "$locked|default": false,
    "$custom_content|default": "Nova_client_button.button_label",
    "bindings": "$button_bindings",
    "$button_bindings|default": [],
    "button_mappings": "$NC_button_mappings",
    "$NC_button_mappings|default": [
      {
        "from_button_id": "button.menu_select",
        "mapping_type": "pressed",
        "to_button_id": "button.menu_select"
      },
      {
        "from_button_id": "button.menu_ok",
        "mapping_type": "focused",
        "to_button_id": "button.menu_ok"
      },
      {
        "from_button_id": "button.menu_left",
        "ignored": "(not $enable_directional_toggling)",
        "input_mode_condition": "gamepad_and_not_gaze",
        "mapping_type": "pressed",
        "to_button_id": "$toggle_off_button"
      },
      {
        "from_button_id": "button.menu_right",
        "ignored": "(not $enable_directional_toggling)",
        "input_mode_condition": "gamepad_and_not_gaze",
        "mapping_type": "pressed",
        "to_button_id": "$toggle_on_button"
      },
      {
        "ignored": "(not $enable_hover_event_mapping)",
        "mapping_type": "pressed",
        "to_button_id": "$toggle_hover_event_mapping"
      }
    ],
    "controls": [
      {
        "custom_content@$custom_content": {}
      }
    ]
  },
  "button_content_icon": {
    "type": "image",
    "$default|default": false,
    "$hover|default": false,
    "$pressed|default": false,
    "bindings": "$button_bindings",
    "$button_bindings|default": [],
    "controls": [
      {
        "label@Nova_client_button.label_icon": {}
      }
    ]
  },
  "label_icon": {
    "type": "panel",
    "layer": 15,
    "$NC_icon|default": "realms",
    "$NC_button_text|default": "Â§cMissing Text",
    "controls": [
      {
        "icon": {
          "type": "image",
          "texture": "('textures/ui/sidebar_icons/' + $NC_icon)",
          "layer": 13,
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "size": [
            16,
            16
          ],
          "offset": [
            3,
            1
          ]
        }
      },
      {
        "label": {
          "type": "label",
          "text": "$NC_button_text",
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "layer": 13,
          "color": [
            1,
            1,
            1
          ],
          "offset": [
            24,
            1.3
          ]
        }
      }
    ]
  },
  "button_icon": {
    "layer": 2,
    "texture": "$button_icon",
    "type": "image",
    "size": "$button_icon_size",
    "$button_icon_size|default": [
      15,
      15
    ],
    "bindings": "$button_bindings",
    "$button_bindings|default": []
  },
  "button_label": {
    "type": "label",
    "text": "$button_text",
    "shadow": "$shadow",
    "$shadow|default": false,
    "font_scale_factor": "$font_scale_factor",
    "$font_scale_factor|default": 1,
    "layer": 10,
    "font_type": "$font_type",
    "$font_type|default": "default",
    "size": [
      "default",
      "default"
    ],
    "color": "$text_color",
    "$text_color|default": [
      0.3,
      0.3,
      0.3
    ],
    "offset": "$button_text_offset",
    "$button_text_offset|default": [
      0,
      0
    ]
  },
  "label_hover_text": {
    "type": "panel",
    "$button_content_hover_text|default": "Nova_client_button.button_label",
    "controls": [
      {
        "1@Nova_Client_common.hover_text_template": {}
      },
      {
        "2@$button_content_hover_text": {}
      }
    ]
  }
}