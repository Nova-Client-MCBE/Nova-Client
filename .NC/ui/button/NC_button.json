  "white_border@common.button": {
    "locked_control": "locked",
    "sound_volume": 0.0,
    "controls": [
      {
        "default@Nova_client_button.default": {
          "$default_texture": "textures/ui/Black",
          "alpha": 0.4,
          "$text_color": [
            1,
            1,
            1
          ]
        }
      },
      {
        "hover@Nova_client_button.hover": {
          "$hover_texture": "textures/ui/focus_border_white",
          "$text_color": [
            1,
            1,
            1
          ]
        }
      },
      {
        "pressed@Nova_client_button.pressed": {
          "$pressed_texture": "textures/ui/focus_border_white",
          "$text_color": [
            1,
            1,
            1
          ]
        }
      },
      {
        "locked@Nova_client_button.locked": {
          "$locked_texture": "textures/ui/Black",
